<mvc:View
		controllerName="com.diegodossantos95.MMPSFrontend.controller.Master"
		xmlns:mvc="sap.ui.core.mvc"
		displayBlock="true"
		xmlns="sap.m">
    <Page id="masterPage" title="Master">
        <content>
            <List items="{Master>/}">
                <items>
                    <StandardListItem title="{Master>name}" type="Navigation"/>
                </items>
            </List>
        </content>
        <footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button icon="sap-icon://add"/>
			</Toolbar>
		</footer>
    </Page>
</mvc:View>
