<mvc:View
		controllerName="com.diegodossantos95.MMPSFrontend.controller.Master"
		xmlns:mvc="sap.ui.core.mvc"
		displayBlock="true"
		xmlns="sap.m">
    <Page id="masterPage">
        <customHeader>
			<Toolbar>
				<Button icon="sap-icon://home" press="onHomePress" />
				<ToolbarSpacer/>
				<Title text="Master" level="H2"/>
                <ToolbarSpacer/>
			</Toolbar>
		</customHeader>
        <content>
            <List items="{Master>/}">
                <items>
                    <StandardListItem title="{Master>name}" type="Navigation" press="onItemPress"/>
                </items>
            </List>
        </content>
        <footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button icon="sap-icon://add" press="onCreatePress"/>
			</Toolbar>
		</footer>
    </Page>
</mvc:View>
