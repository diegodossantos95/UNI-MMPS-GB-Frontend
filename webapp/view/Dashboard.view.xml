<mvc:View controllerName="com.diegodossantos95.MMPSFrontend.controller.Dashboard"
		  xmlns:mvc="sap.ui.core.mvc"
		  xmlns="sap.m"
          xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
          displayBlock="true">
    <Page title="Dashboard">
        <TileContainer tiles="{Dashboard>/tiles}">
            <StandardTile
                    icon="{Dashboard>icon}"
                    type="Create"
                    title="{Dashboard>title}"
                    press="onTilePress"
                    customData:view="{Dashboard>view}"/>
        </TileContainer>
    </Page>
</mvc:View>