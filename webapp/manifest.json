{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.diegodossantos95.MMPSFrontend",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"ach": "XX"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize",
            "sap_belize_plus"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "com.diegodossantos95.MMPSFrontend.view.App",
			"type": "XML",
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.diegodossantos95.MMPSFrontend.i18n.i18n"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.diegodossantos95.MMPSFrontend.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
                {
                  "pattern": "",
                  "name": "Dashboard",
                  "target": "Dashboard"
                },{
                    "pattern": "{entity}",
                    "name": "SplitApp",
                    "target": ["SplitApp", "Master", "Empty"]
                },{
                    "pattern": "{id}",
                    "name": "Detail",
                    "parent": "SplitApp",
                    "target": ["Master", "Detail"]
                }],
			"targets": {
                "Dashboard": {
                  "viewName": "Dashboard",
                  "viewLevel": 1
                },
                "SplitApp": {
                   "viewName": "SplitApp"
                },
                "Master": {
                  "parent" : "SplitApp",
                  "viewName": "Master",
                  "viewLevel": 2, 
                  "controlId": "idSplitAppControl",
                  "controlAggregation": "masterPages"
                },
                "Detail": {
                  "parent" : "SplitApp",
                  "viewName": "Detail",
                  "viewLevel": 3,
                  "controlId": "idSplitAppControl",
                  "controlAggregation": "detailPages"
                },
                "Empty": {
                  "parent" : "SplitApp",
                  "viewName": "Empty",
                  "viewLevel": 2,
                  "controlId": "idSplitAppControl",
                  "controlAggregation": "detailPages"
                },
                "Create": {
                  "parent" : "SplitApp",
                  "viewName": "Create",
                  "viewLevel": 3,
                  "controlId": "idSplitAppControl",
                  "controlAggregation": "detailPages"
                }
			}
		}
	}
}
